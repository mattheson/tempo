{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "dmg",
    "resources": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "frameworks": [],
      "exceptionDomain": "",
      "signingIdentity": "-",
      "providerShortName": null,
      "entitlements": null
    }
  },
  "productName": "Tempo",
  "identifier": "com.taniguchi.tempo",
  "plugins": {},
  "app": {
    "security": {
      "capabilities": ["tempo"],
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": ["/*/**"]
      }
    }
  }
}
